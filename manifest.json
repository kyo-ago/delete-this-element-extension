{
    "manifest_version": 3,
    "name": "Delete this element",
    "version": "1.0",
    "description": "__MSG_extensionDescription__",
    "permissions": ["contextMenus"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
    "default_locale": "en",
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }